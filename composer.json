{
  "require": {
    "guzzlehttp/guzzle": "@stable",
    "phpunit/dbunit": "@stable",
    "phpunit/phpunit": "@stable",
    "swiftmailer/swiftmailer": "@stable",
    "lcobucci/jwt": "3.2",
    "ramsey/uuid": "@stable",
    "phpdocumentor/phpdocumentor": "dev-master"
  },

  "scripts":{
    "apigen": "./vendor/bin/apigen generate ./php/classes/*.php --destination doc",
    "phpunit": "./vendor/bin/phpunit -c ./php/classes/Test --coverage-html coverage",
    "apiUnit": "./vendor/bin/phpunit -c ./php/classes/ApiTest --coverage-html coverage",
    "phpdoc": "./vendor/bin/phpdoc run -d ./php/classes -t doc"
  }
}